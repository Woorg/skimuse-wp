{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "publicPath": "/app/themes/skimuse/",
  "devUrl": "http://skimuse.loc",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]",
  "watch": [
    "app/**/*.php",
    "config/**/*.php",
    "resources/views/*.php",
    "resources/views/**/*.php",
    "resources/front/static/dev/assets/**/*.css",
    "resources/front/static/dev/assets/**/*.js",
    "resources/front/static/dev/assets/*.js"
  ]
}
